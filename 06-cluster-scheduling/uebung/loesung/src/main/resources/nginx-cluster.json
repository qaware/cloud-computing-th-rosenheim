{
  "id": "nginx-cluster-meinport",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "qaware/nginx",
      "network": "BRIDGE",
      "portMappings": [
        { "hostPort": 8077, "containerPort": 80, "protocol": "tcp"}
      ]
    }
  },
  "acceptedResourceRoles": ["slave_public"],
  "instances": 1,
  "cpus": 0.1,
  "mem": 64
}